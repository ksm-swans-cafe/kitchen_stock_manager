generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type CartCartLunchbox {
  /// Multiple data types found: Int: 92.9%, BigInt: 7.1% out of 14 sampled entries
  lunchbox_limit      Json
  lunchbox_menu       CartCartLunchboxLunchboxMenu[]
  lunchbox_name       String
  lunchbox_set_name   String
  /// Multiple data types found: Int: 92.9%, BigInt: 7.1% out of 14 sampled entries
  lunchbox_total      Json
  lunchbox_total_cost BigInt?
}

type CartCartLunchboxLunchboxMenu {
  menu_category    String
  menu_cost        BigInt?
  menu_description CartCartLunchboxLunchboxDescription[]
  menu_ingredients CartCartLunchboxLunchboxMenuMenuIngredients[]
  menu_name        String
  /// Multiple data types found: Int: 93.8%, BigInt: 6.3% out of 32 sampled entries
  menu_order_id    Json
  menu_subname     String
  /// Multiple data types found: Int: 93.8%, BigInt: 6.3% out of 32 sampled entries
  menu_total       Json
}

type CartCartLunchboxLunchboxDescription {
  menu_description_id String?
  menu_description_title String
  menu_description_value String
}

type CartCartLunchboxLunchboxMenuMenuIngredients {
  ingredient_name   String
  ingredient_status Boolean?
  /// Multiple data types found: Int: 95.2%, BigInt: 4.8% out of 146 sampled entries
  useItem           Json
}

type CartCartMenuItems {
  menu_description String?
  menu_ingredients CartCartMenuItemsMenuIngredients[]
  menu_name        String
  menu_notes       CartCartMenuItemsMenuNotes[]
  menu_order_id    Int?
  /// Multiple data types found: Int: 98.8%, BigInt: 1.2% out of 683 sampled entries
  menu_total       Json
}

type CartCartMenuItemsMenuIngredients {
  ingredient_name   String?
  ingredient_status Boolean?
  /// Multiple data types found: Int: 98.6%, BigInt: 1.4% out of 2659 sampled entries
  useItem           Json?
}

type CartCartMenuItemsMenuNotes {
  note String
  qty  Int
}

type MenuMenuIngredients {
  ingredient_name String
  /// Multiple data types found: Int: 98.8%, BigInt: 1.2% out of 337 sampled entries
  useItem         Json
}

type MenuMenuLunchbox {
  lunchbox_menuid String?
  lunchbox_cost     BigInt?
  lunchbox_name     String
  lunchbox_set_name String
  lunchbox_menu_category String?
  lunchbox_showPrice Boolean?
  lunchbox_AutoRice Boolean?
}

type CartCartDescription {
  description_id     String?
  description_title  String
  description_value  String
}

model cart {
  id                 String              @id @default(auto()) @map("_id") @db.ObjectId
  cart_create_date   String
  cart_channel_access String
  cart_customer_name String
  cart_customer_tel  String
  cart_delivery_date String
  cart_export_time   String
  cart_id            String
  cart_last_update   String?
  cart_location_send String
  cart_lunchbox      CartCartLunchbox[]
  cart_total_cost_lunchbox String
  cart_order_number  String
  cart_receive_time  String
  cart_receive_name  String
  cart_shipping_cost String
  cart_status        String
  cart_username      String
  cart_pay_type      String
  cart_pay_deposit   String
  cart_pay_isdeposit Boolean
  cart_pay_charge    String
  cart_pay_cost      String
  cart_ispay         String
  cart_invoice_tex  String
  cart_total_cost    String
  cart_total_remain  String
  cart_description   CartCartDescription[]
  cart_pinned        Boolean?
}

model cart_log {
  id                 String              @id @default(auto()) @map("_id") @db.ObjectId
  message String
  status String
  create_date String
  create_by String
}

model employee {
  id                 String @id @default(auto()) @map("_id") @db.ObjectId
  employee_firstname String
  employee_id        String
  employee_lastname  String
  employee_pin       String
  employee_role      String
  employee_username  String
}

model ingredient_transaction {
  id                        String @id @default(auto()) @map("_id") @db.ObjectId
  ingredient_name           String
  transaction_date          String
  transaction_from_username String
  transaction_id            Int
  transaction_quantity      String
  transaction_total_price   String
  transaction_type          String
  transaction_units         String
}

model ingredients {
  id                        String @id @default(auto()) @map("_id") @db.ObjectId
  ingredient_category       String
  ingredient_id             Int
  ingredient_image          String
  ingredient_lastupdate     String
  ingredient_name           String
  ingredient_price          String
  ingredient_price_per_unit String
  ingredient_sub_category   String
  ingredient_total          String
  ingredient_total_alert    String
  ingredient_unit           String
}

model lunchbox {
  id                      String  @id @default(auto()) @map("_id") @db.ObjectId
  lunchbox_limit          Int
  lunchbox_name           String
  lunchbox_set_name       String
  lunchbox_name_image     String?
  lunchbox_set_name_image String?
}

model menu {
  id               String                @id @default(auto()) @map("_id") @db.ObjectId
  menu_category    String
  /// Multiple data types found: Int: 99.1%, BigInt: 0.9% out of 110 sampled entries
  menu_cost        Json
  menu_id          Int                   @unique
  menu_image       String
  menu_ingredients MenuMenuIngredients[]
  menu_lunchbox    MenuMenuLunchbox[]
  menu_name        String
  menu_subname     String
}
