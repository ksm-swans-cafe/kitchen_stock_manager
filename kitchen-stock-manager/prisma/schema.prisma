generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type CartCartLunchbox {
  /// Multiple data types found: Int: 90%, BigInt: 10% out of 10 sampled entries
  lunchbox_limit      Json
  lunchbox_menu       CartCartLunchboxLunchboxMenu[]
  lunchbox_name       String
  lunchbox_set_name   String
  /// Multiple data types found: Int: 90%, BigInt: 10% out of 10 sampled entries
  lunchbox_total      Json
  lunchbox_total_cost BigInt?
}

type CartCartLunchboxLunchboxMenu {
  menu_category    String
  menu_cost        BigInt?
  menu_description String
  menu_ingredients CartCartLunchboxLunchboxMenuMenuIngredients[]
  menu_name        String
  /// Multiple data types found: Int: 90%, BigInt: 10% out of 20 sampled entries
  menu_order_id    Json
  menu_subname     String
  /// Multiple data types found: Int: 90%, BigInt: 10% out of 20 sampled entries
  menu_total       Json
}

type CartCartLunchboxLunchboxMenuMenuIngredients {
  ingredient_name String
  /// Multiple data types found: Int: 92.1%, BigInt: 7.9% out of 89 sampled entries
  useItem         Json
}

type CartCartMenuItems {
  menu_description String?
  menu_ingredients CartCartMenuItemsMenuIngredients[]
  menu_name        String
  menu_subname     String?
  menu_category    String?
  menu_notes       CartCartMenuItemsMenuNotes[]
  menu_order_id    Int?
  menu_total       Int
}

type CartCartMenuItemsMenuIngredients {
  ingredient_name   String
  ingredient_status Boolean?
  useItem           Int
}

type CartCartMenuItemsMenuNotes {
  note String
  qty  Int
}

type MenuMenuIngredients {
  ingredient_name String
  useItem         Int
}

type MenuMenuLunchbox {
  lunchbox_name     String
  lunchbox_set_name String
}

model cart {
  id                 String              @id @default(auto()) @map("_id") @db.ObjectId
  cart_create_date   String
  cart_customer_name String
  cart_customer_tel  String
  cart_delivery_date String
  cart_export_time   String
  cart_id            String
  cart_last_update   String?
  cart_location_send String
  cart_lunchbox      CartCartLunchbox[]
  cart_menu_items    CartCartMenuItems[]
  cart_order_number  String
  cart_receive_time  String
  cart_shipping_cost String
  cart_status        String
  cart_username      String
}

model employee {
  id                 String @id @default(auto()) @map("_id") @db.ObjectId
  employee_firstname String
  employee_id        String
  employee_lastname  String
  employee_pin       String
  employee_role      String
  employee_username  String
}

model ingredient_transaction {
  id                        String @id @default(auto()) @map("_id") @db.ObjectId
  ingredient_name           String
  transaction_date          String
  transaction_from_username String
  transaction_id            Int
  transaction_quantity      String
  transaction_total_price   String
  transaction_type          String
  transaction_units         String
}

model ingredients {
  id                        String @id @default(auto()) @map("_id") @db.ObjectId
  ingredient_category       String
  ingredient_id             Int
  ingredient_image          String
  ingredient_lastupdate     String
  ingredient_name           String
  ingredient_price          String
  ingredient_price_per_unit String
  ingredient_sub_category   String
  ingredient_total          String
  ingredient_total_alert    String
  ingredient_unit           String
}

model lunchbox {
  id                String @id @default(auto()) @map("_id") @db.ObjectId
  lunchbox_limit    Int
  lunchbox_name     String
  lunchbox_set_name String
}

model menu {
  id               String                @id @default(auto()) @map("_id") @db.ObjectId
  menu_category    String
  menu_cost        Int
  menu_id          Int                   @unique
  menu_image       String
  menu_ingredients MenuMenuIngredients[]
  menu_lunchbox    MenuMenuLunchbox[]
  menu_name        String
  menu_subname     String
}
